w1:Label("Misc")
w1:Button(
    "Infinite Stamina",
    function()
        local old
local path = game:GetService("Workspace")
old = hookmetamethod(path, "__index", function(instances,property)
   if tostring(instances) == "IntValue_Stamina" and property == "Value" then
       return math.huge
   end
   return old(instances,property)
end)
    end
)
w1:Button(
    "Infinite Ammo",
    function()
        local old
local path = game.Players.LocalPlayer.Backpack
old = hookmetamethod(path, "__index", function(instances,property)
   if tostring(instances) == "IntValue_CurrentMagazine" and property == "Value" then
       return math.huge
   end
   return old(instances,property)
end)
    end
)
